<h1>Listing commits</h1>

<table>
  <tr>
    <th>Committable type</th>
    <th>Committable</th>
    <th>Identifier</th>
    <th>Vcs reference</th>
    <th>Vcs diff</th>
    <th>Vcs info</th>
    <th>Vcs affected files</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @commits.each do |commit| %>
  <tr>
    <td><%= commit.committable_type %></td>
    <td><%= commit.committable_id %></td>
    <td><%= commit.identifier %></td>
    <td><%= commit.vcs_reference %></td>
    <td><%= commit.vcs_diff %></td>
    <td><%= commit.vcs_info %></td>
    <td><%= commit.vcs_affected_files %></td>
    <td><%= link_to 'Show', commit %></td>
    <td><%= link_to 'Edit', edit_commit_path(commit) %></td>
    <td><%= link_to 'Destroy', commit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Commit', new_commit_path %>
